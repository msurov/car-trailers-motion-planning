\begin_layout Standard
\begin_inset Formula $\tan\phi=\frac{\dot{\theta}_{0}}{u_{1}}=\frac{\dot{\theta}_{0}}{\cos\theta_{0}\dot{x}+\sin\theta_{0}\dot{y}}$
\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula $\theta_{n}=\theta_{n}\left(v_{n}\right)$
\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula $\dot{\theta}_{n}=\dot{\theta}_{n}\left(v_{n},\dot{v}_{n}\right)$
\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula $v_{n-1}=v_{n-1}\left(v_{n},\dot{v}_{n}\right)$
\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula $\theta_{n-1}=\theta_{n-1}\left(v_{n},\ddot{v}_{n}\right)$
\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula $\theta_{0}=\theta_{0}\left(v_{n},\dot{v}_{n},\dots,v_{n}^{\left(n\right)}\right)$
\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula $\dot{\theta}_{0}=\dot{\theta}_{0}\left(v_{n},\dot{v}_{n},\dots,v_{n}^{\left(n+1\right)}\right)$
\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula $v_{0}=v_{0}\left(v_{n},\dot{v}_{n},\dots,v_{n}^{\left(n\right)}\right)$
\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula $u_{1}=u_{1}\left(\theta_{0},v_{0}\right)=u_{1}\left(v_{n},\dot{v}_{n},\dots,v_{n}^{\left(n\right)}\right)$
\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula $\phi=\phi\left(\dot{\theta}_{0},u_{1}\right)=\phi\left(v_{n},\dot{v}_{n},\dots,v_{n}^{\left(n+1\right)}\right)$
\end_inset


\end_layout

